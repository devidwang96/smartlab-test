{
    "private": true,
    "scripts": {
        "dev": "npm run development",
        "development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=./webpack.config.js",
        "watch": "concurrently --kill-others  \"npm run watch-client\" \"npm run watch-server\" ",
        "watch-server": "cross-env VUE_ENV=server NODE_ENV=development node_modules/webpack/bin/webpack.js --watch --progress --hide-modules --config=./webpack.config.js",
        "watch-client": "cross-env VUE_ENV=client NODE_ENV=development node_modules/webpack/bin/webpack.js --watch --progress --hide-modules --config=./webpack.config.js",
        "watch-poll": "npm run watch -- --watch-poll",
        "hot": "cross-env NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js",
        "prod": "npm run production",
        "production": "npm run prod:server && npm run prod:client",
        "prod:client": "cross-env VUE_ENV=client NODE_ENV=production node_modules/webpack/bin/webpack.js --progress --hide-modules --config=./webpack.config.js",
        "prod:server": "cross-env VUE_ENV=server NODE_ENV=production node_modules/webpack/bin/webpack.js --progress --hide-modules --config=./webpack.config.js"
    },
    "dependencies": {
        "autoprefixer": "^9.1.0",
        "axios": "^0.18",
        "bootstrap": "3.3.5",
        "bootstrap-notify": "^3.1.3",
        "bootstrap-select": "^1.13.1",
        "cross-env": "^5.1",
        "jquery": "^3.2",
        "laravel-mix": "0.*",
        "lodash": "^4.17.4",
        "moment-timezone": "^0.5.17",
        "popper.js": "^1.12",
        "postcss-loader": "^3.0.0",
        "promise-polyfill": "^8.0.0",
        "vee-validate": "^2.0.0-rc.17",
        "vue": "^2.5.7",
        "vue-flatpickr-component": "^5.0.0",
        "vue-resource": "^1.3.4",
        "vue-router": "^3.0.1",
        "vue-scrollto": "^2.8.0",
        "vue-server-renderer": "^2.5.16",
        "vue-the-mask": "^0.11.1",
        "vue-yandex-maps": "^0.7.12",
        "vue2-google-maps": "^0.10.1",
        "vuex": "^2.3.1",
        "vuex-router-sync": "^5.0.0"
    },
    "devDependencies": {
        "autoprefixer-loader": "^3.2.0",
        "babel-plugin-syntax-dynamic-import": "^6.18.0",
        "babel-plugin-transform-object-rest-spread": "^6.23.0",
        "babel-plugin-transform-runtime": "^6.23.0",
        "babel-preset-es2015": "^6.24.1",
        "babel-preset-stage-2": "^6.24.1",
        "bootstrap-sass": "^3.3.7",
        "concurrently": "^3.5.1",
        "cross-env": "^5.0.1",
        "path": "^0.12.7",
        "webpack-isomorphic-tools": "^3.0.5",
        "webpack-node-externals": "^1.6.0"
    }
}
